// 在文档中了解更多: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// 用户
model List {
  /// 自增主键
  id    Int     @id @default(autoincrement())
  /// 邮箱
  email String  @unique
  /// 昵称
  name  String?

  @@map("list")
}
