// 在文档中了解更多: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// 用户
model List {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String? /// 昵称

  @@map("list")
}
